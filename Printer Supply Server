Imports Microsoft.Office.Interop
Imports System.Data.OleDb.OleDbConnection
Imports System.Data.OleDb.OleDbCommand
Imports WindowsApplication2.Form2
Imports System.Data.OleDb
Imports System.Net.Mail

Public Class Form1
    Public ConnString As String = "Provider=Microsoft.ACE.OLEDB.12.0;Data Source=\\AUHEBWFS20\hebron\Groups\Information Technology\Databases\Russ's\Printer Supplies Inventory.accdb"

    Public Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click
        Button2.Enabled = False
        Button4.Enabled = False
        Button3.Enabled = False
        Me.Cursor = Cursors.WaitCursor
        'Select the table and read it.
        Dim cmdfill As New OleDbCommand("SELECT * FROM Printer", Globals.Cnn)
        Dim datareader As OleDbDataReader
        Dim add As Object
        Dim addx As Object
        Dim sum As Object
        add = 0
        Globals.Cnn.Open()

        datareader = cmdfill.ExecuteReader
        'Select The table and read it.


        'Add to Inventory

        'Add to Inventory

        If ListBox1.SelectedIndex = 0 Then
            add = InputBox("How Much Do You Want To Add?", "Add", "")
            If add = "" Then
                Globals.Cnn.Close()
                Me.Cursor = Cursors.Default
                Exit Sub
            End If
            While datareader.Read
                Globals.Supply = datareader("HPLJ4100")
            End While
            addx = Globals.Supply
            sum = add - -addx
            Globals.Com.CommandText = "UPDATE Printer SET HPLJ4100 ='" & +sum & "'"
            Globals.Com.ExecuteNonQuery()
            Globals.Cnn.Close()
            MySaveGame.global_hp_lj_4100 = Globals.Supply
            MySaveGame.global_hp_lj_4100 += 1
        End If

        If ListBox1.SelectedIndex = 1 Then
            add = InputBox("How Much Do You Want To Add?", "Add", "")
