    Private Sub Timer3_Tick(sender As Object, e As EventArgs) Handles Timer3.Tick
        'Reset Monthly reports
        Dim Day = Today.Day
        If Day = 2 Then
            Globals.report = 0
            Globals.report_done = 0
            Timer1.Enabled = True
            Timer4.Enabled = True
        End If
    End Sub

