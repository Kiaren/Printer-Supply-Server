        Globals.Cnn.Close()
        Button2.Enabled = True
        Button4.Enabled = True
        Button3.Enabled = True
    End Sub

    Public Sub Button3_Click(sender As Object, e As EventArgs)
        'Debug Message to see if it's adding/subtracting
        MessageBox.Show(MySaveGame.global_hp_lj_4100)
    End Sub

    Public Sub Button2_Click(sender As Object, e As EventArgs) Handles Button2.Click
        Button1.Enabled = False
        Button4.Enabled = False
        Button3.Enabled = False
        Me.Cursor = Cursors.WaitCursor
        Me.Refresh()
        Dim cmdfill As New OleDbCommand("SELECT * FROM Printer", Globals.Cnn)
        Dim cmdfill_used As New OleDbCommand("SELECT * FROM MonthlyUsed", Globals.Cnn)
        Dim datareader As OleDbDataReader
        Dim datareader2 As OleDbDataReader
        Globals.Cnn.Open()

        datareader = cmdfill.ExecuteReader
        datareader2 = cmdfill_used.ExecuteReader


        'Subtract From Inventory
        'hp lj 4100
        If ListBox1.SelectedIndex = 0 Then
            While datareader.Read
                Globals.Supply = datareader("HPLJ4100")
            End While
            While datareader2.Read
                Globals.Used = datareader2("HPLJ4100")
            End While
            Globals.Com.CommandText = "UPDATE Printer SET HPLJ4100 ='" & Globals.Supply - 1 & "'"
            Globals.Com.ExecuteNonQuery()
            Globals.Com.CommandText = "UPDATE MonthlyUsed SET HPLJ4100 ='" & Globals.Used + 1 & "'"
            Globals.Com.ExecuteNonQuery()
            Globals.Cnn.Close()
            MySaveGame.global_hp_lj_4100 = Globals.Supply
            MySaveGame.global_hp_lj_4100 -= 1
        End If

        If ListBox1.SelectedIndex = 1 Then
            While datareader.Read
                Globals.Supply = datareader("HPLJ4015DN")
            End While
            While datareader2.Read
                Globals.Used = datareader2("HPLJ4015DN")
            End While
            Globals.Com.CommandText = "UPDATE Printer SET HPLJ4015DN ='" & Globals.Supply - 1 & "'"
