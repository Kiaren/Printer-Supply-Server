                Globals.Used = datareader2("HP933XLMAGENTA")
            End While
            Globals.Com.CommandText = "UPDATE Printer SET HP933XLMAGENTA = '" & Globals.Supply - 1 & "'"
            Globals.Com.ExecuteNonQuery()
            Globals.Com.CommandText = "UPDATE MonthlyUsed SET HP933XLMAGENTA = '" & Globals.Used + 1 & "'"
            Globals.Com.ExecuteNonQuery()
            Globals.Cnn.Close()
            Globals.global_hp_933xl_magenta = Globals.Supply
            Globals.global_hp_933xl_magenta -= 1
        End If

        If ListBox1.SelectedIndex = 33 Then
            While datareader.Read
                Globals.Supply = datareader("HP933XLYELLOW")
            End While
            While datareader2.Read
                Globals.Used = datareader2("HP933XLYELLOW")
            End While
            Globals.Com.CommandText = "UPDATE Printer SET HP933XLYELLOW = '" & Globals.Supply - 1 & "'"
            Globals.Com.ExecuteNonQuery()
            Globals.Com.CommandText = "UPDATE MonthlyUsed SET HP933XlYELLOW = '" & Globals.Used + 1 & "'"
            Globals.Com.ExecuteNonQuery()
            Globals.Cnn.Close()
            Globals.global_hp_933xl_yellow = Globals.Supply
            Globals.global_hp_933xl_yellow -= 1
        End If

